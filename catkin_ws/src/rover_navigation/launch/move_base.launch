<?xml version="1.0"?>

<launch>

   <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find rover_navigation)/config/costmap_common.yaml" command="load" ns="global_costmap" /> 
    <rosparam file="$(find rover_navigation)/config/costmap_common.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find rover_navigation)/config/local_costmap.yaml" command="load" />
    <rosparam file="$(find rover_navigation)/config/global_costmap.yaml" command="load" /> 
    <rosparam file="$(find rover_navigation)/config/base_local_planner.yaml" command="load" />
    <remap from="cmd_vel" to="rover/cmd_vel"/>
 </node>

</launch> 
<!-- Reff : http://wiki.ros.org/navigation/Tutorials/RobotSetup -->